@model List<People> 

<h1>
  this is index file.
</h1>
<table class="table table-primary">
  <thead>

  
    <tr class="table-active">
    </tr>
    <tr>
      
       <td  class="table-active">SN</td>
      <td  class="table-active">Firstname</td>
      <td  class="table-active">lastname</td>
      <td class="table-active">Address</td>
      <td class="table-active">Gender</td>
      <td class="table-active">Salary</td>
    </tr>
</thead>

<tbody>
  @for (int i=0;i<Model.Count;i++)
  {
    <tr><td>@(i+1)</td>
      <td>
        <a href="/employees/details?firstname=@Model[i].Firstname">@Model[i].Firstname</a>
        </td>
     <td>@Model[i].SN </td>
      <td>@Model[i].Lastname </td>
      <td>@Model[i].Address</td>
      <td>@Model[i].Gender</td>
      <td>@Model[i].Salary</td>
    </tr>
  }

</tbody>

</table>